	"version": "0.2.0",
	"configurations": [
	{
		"name": "(lldb) test",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/a.out",
		"args": ["1 5", "2"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "lldb"
	},
	{
		"name": "(lldb) checker",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/checker",
		"args": ["-v", "9 1 7 2 4 8 5 10 6 3"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"MIMode": "lldb"
	},
	{
		"name": "(lldb) push_swap",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/push_swap",
		"args": ["5 6 2 9 1 3"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"internalConsoleOptions": "openOnFirstSessionStart",
		"MIMode": "lldb"
	}
	]
}