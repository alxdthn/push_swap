	"version": "0.2.0",
	"configurations": [
	{
		"name": "(lldb) test",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/a.out",
		"args": ["1", "5", "50", "12"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"MIMode": "lldb"
	},
	{
		"name": "(lldb) checker",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/checker",
		"args": ["-v", "1 2 3 4"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"MIMode": "lldb"
	},
	{
		"name": "(lldb) push_swap",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/push_swap",
		"args": ["1 2 7 6 3 4 5"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"internalConsoleOptions": "openOnFirstSessionStart",
		"MIMode": "lldb"
	}
	]
}